#CPPFLAGS = -O3 -g -ffast-math -march=native -pthread
CPPFLAGS = -O3 -g -ffast-math -march=native -fstrict-aliasing -pthread
CPPFLAGS_ASM = -O3 -ffast-math -march=native -fstrict-aliasing -S -pthread

all : main

main : main1.cpp
	g++ $^ -o $@ $(CPPFLAGS)

asm : main1.cpp
	g++ $^ $(CPPFLAGS_ASM)

clean: 
	@rm -f main
	@rm -f main.s
